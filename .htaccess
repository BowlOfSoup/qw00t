<IfModule mod_rewrite.c>
    Options -MultiViews

    RewriteEngine On
    RewriteBase /

    RewriteCond %{REQUEST_URI} ^/doc
    RewriteRule (.*) doc/index.html [QSA,L]

    RewriteCond %{REQUEST_URI} ^/api/
    RewriteRule (.*) app/bootstrap.php [QSA,L]

    RewriteCond %{REQUEST_URI} ^/$
    RewriteRule ^(.*)$ /web/$1 [QSA,L]
</IfModule>
